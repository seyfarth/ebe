a_byte: db      1                   ; define byte
more:   db      0x0a, 10001001b
short:  dw      125                 ; define word          2 bytes
shorts: dw      1, 2, 3
int:    dd      0x10001001          ; define double word   4 bytes
long:   dq      0x1000100110101011  ; define quad-word     8 bytes
