#   cout << "i is " << i << endl;
        .section .data
format: db      "i is %d". 0x0a, 0  ; Have to add new-line and 0
        .section .text
        lea     format, %eax        ; parameter 1 to printf
        mov     %eax, newPar1(%esp) ; assuming use of frame macro
        mov     i, %eax             ; get value of i for parameter 2
        mov     %eax, newPar2(%esp)
        call    printf

